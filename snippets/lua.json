{
  "Anonymous Function": {
    "prefix": "afu",
    "body": [
      "function($1)",
      " ${0:$TM_SELECTED_TEXT}",
      "end"
    ],
    "desc": "Create an anonymous function"
  },
  "Foreach in array": {
    "prefix": "ifor",
    "body": [
      "for ${1:i}, ${2:v} in ipairs($3) do",
      "  ${0:$TM_SELECTED_TEXT}",
      "end"
    ]
  },
  "Foreach in table": {
    "prefix": "pfor",
    "body": [
      "for ${1:k}, ${2:v} in pairs($3) do",
      "  ${0:$TM_SELECTED_TEXT}",
      "end"
    ]
  },
  "Module": {
    "prefix": "mod",
    "body": [
      "local ${1:M} = {}",
      "$0"
    ]
  }
}
